3)UPDATE employee
SET name = 'Soner'
WHERE name = 'Yeliz';

UPDATE employee
SET birthday = '1991-09-18'
WHERE birthday = '2002-01-04'

UPDATE employee
SET id = 34
WHERE id = 10;

